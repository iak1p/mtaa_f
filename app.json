{
  "expo": {
    "name": "my-app",
    "slug": "my-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": ["expo-location"],
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#FFFFFF",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Нужен доступ к геопозиции"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "web": {}
  }
}
